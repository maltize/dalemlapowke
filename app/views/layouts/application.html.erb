      <h1>dalemlapowke.pl</h1>
    </div>

    <div class="row">
      <div class="span12">
        <%= yield %>
      </div>
    </div>

    <footer>
      <p>© dalemlapowke.pl 2013</p>
    </footer>
  </div>
</body>
</html>
